  <section class="portfolio-item-details">
    <div class="container">
      <div class="row item-title">
        <div class="col-sm-9">
          <h1>새로운 멤버 등록하기</h1>
          <div class="categories">
            <a></a>
          </div>
        </div>

        <div class="col-sm-3">
          <div class="text-right">
            <div class="item-detail">
            </div>
            <div class="item-detail">
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <section class="portfolio-container">
    <div class="container">
      <div class="panel panel-primary">
        <div class="panel-body">
          <%= form_for [current_club, :admin, @user], html: {role: "form", class: "form-horizontal"} do |f| %>
            <%= render partial: 'form', locals: {f: f} %>
            <div class="pull-right">
              <%= f.button type: :submit, class: "btn btn-blue btn-lg btn-icon" do %>
                등록<i class="entypo-check"></i>
              <% end %>
              <%= link_to club_admin_users_path(current_club) do %>
                <button type="button" class="btn btn-gold btn-lg btn-icon">
                  취소<i class="entypo-cancel"></i>
                </button>
              <% end %>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </section>

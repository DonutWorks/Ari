  <section class="portfolio-item-details">
    <div class="container">
      <div class="row item-title">
        <div class="col-sm-9">
          <h1>플로우 수정하기</h1>
          <div class="categories">
            <a></a>
          </div>
        </div>
        
        <div class="col-sm-3">
          <div class="text-right">
            <div class="item-detail">
            </div>
            <div class="item-detail">
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <section class="portfolio-container">
    <div class="container">
      <%= form_for [current_club, :admin, @notice], html: {role: "form", class: "form-horizontal"} do |f|%>
        <div class="panel panel-primary">
          <div class="panel-body">
            <%= render partial: 'form', locals: {f: f} %>
            <p class="pull-right">
              <%= f.submit "수정", class: "btn btn-info btn-lg"  %>
              <%= link_to [current_club, :admin, @notice] do %>
                <button type="button" class="btn btn-gold btn-lg">
                  취소
                </button>
              <% end %>
            </p>
          </div>
        </div>
      <% end %>
      <%= hidden_field_tag 'notice_type', @notice.notice_type %>
    </div>
  </section>

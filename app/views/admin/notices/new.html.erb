  <section class="portfolio-container">
    <div class="container">
      <%= form_for [current_club, :admin, @notice], html: {role: "form", class: "form-horizontal"} do |f|%>
        <div class="panel panel-primary">
          <div class="panel-body">
            <%= render partial: 'form', locals: {f: f} %>
            <p class="pull-right">
              <%= f.submit "등록", class: "btn btn-blue btn-lg btn-icon"  %>
              <%= link_to club_admin_root_path(current_club) do %>

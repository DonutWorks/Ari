
<h1>관리자 페이지</h1>
<p id="notice"><%= flash[:notice] %></p>

<%= link_to "회원 등록", new_user_registration_path %>
<%= link_to "새 링크 만들기", new_admin_gate_path %>

<% @status_list.each do |status| %>
<div>
<h1>Gate: <%= status[:gate].title %></h1>
<p>Read Users<p>
<ul>
<% status[:read_users].each do |user| %>
  <li><%= user.username %></li>
<% end %>
</ul>
<p>Unread Users</p>
<ul>
<% status[:unread_users].each do |user| %>
  <li><%= user.username %></li>
<% end %>
</ul>
</div>



<% end %>
